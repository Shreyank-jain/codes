#include <bits/stdc++.h>
using namespace std;

int main() {

    int tc;
    cin>>tc;
    cin.ignore();
    
    while(tc--)
    {
        string str,str2;
        getline(cin,str);
        int c[256]={0}; 
        str2="" ;
        for(int i=0;i<str.size();i++)
            c[str[i]]++ ;
        
        for(int i=0;i<str.size();i++)
          {
             if(c[str[i]]>=1)
             {
                 str2+=str[i];
                 c[str[i]]=0;
             }
          }
        cout<<str2<<endl ;
    }
	return 0;
}


----------------------------
Input:
2
geeksforgeeks
geeks for geeks

Output:
geksfor
geks for
