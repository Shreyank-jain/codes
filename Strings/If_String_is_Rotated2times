Input:
2
amazon
azonam
geeksforgeeks
geeksgeeksfor

Output:
1
0

#include <bits/stdc++.h>
using namespace std;
int main() {
    int tc;
    cin>>tc;
    while(tc--){
        string s1,s2,s3;
        cin>>s1;
        cin>>s2;
        s3=s2+s2;
        sl=s1+s1;
        if(s3.find(s1)==2 ||sl.find(s2)==2){
            cout<<"1"<<endl;
        }
    else    
        cout<<"0"<<endl;
    }
	return 0;
}

