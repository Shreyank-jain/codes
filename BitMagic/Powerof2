#include<bits/stdc++.h>
using namespace std;
int main()
{
ios_base::sync_with_stdio(false);
cin.tie(NULL);
int t;
cin >> t;
while(t--)
{
unsigned long long n,temp=0;
cin >> n;
for(unsigned long long i=0;i<=sqrt(n);i++)
{
if(pow(2,i)==n)
temp=1;
}
if(temp==1)
cout << "YES" << "\n";
else
cout << "NO" << "\n";
}
return 0;
}

---------------------------------
Input :
3
1
98
1099511627776

Output :
YES
NO
YES
