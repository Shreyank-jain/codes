#include <iostream>
using namespace std;

void nofz(int arr[],int n)
{
    
}
int main() {
	int tc,n;
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cin>>tc;
    int *arr;
    while(tc--)
    {
       cin>>n;
       int count0=0,count1=0;
       arr=new int[n];
       for(int i=0;i<n;i++)
       {  cin>>arr[i] ; }
       for(int i=0;i<n;i++)
       {
           if(arr[i]==0)
              count0++;
           else if(arr[i]==1)
               count1++;
               
       }
       for(int i=0;i<count0;i++)
         {  arr[i]=0; }

       for(int i=count0;i<(count0+count1);i++)
         {  arr[i]=1; }
       for(int i=(count0+count1);i<n;i++)
         {  arr[i]=2; }
        for(int i=0;i<n;i++) cout<<arr[i]<<" ";
        cout<<"\n";
    }
	return 0;
}
